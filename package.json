{
	"name": "groupify",
	"version": "0.1.0",
	"private": true,
	"packageManager": "pnpm@9.12.3",
	"engines": {
		"node": "22"
	},
	"scripts": {
		"dev": "next dev",
		"build": "DB_URL=\"\" next build",
		"start": "next start",
		"lint": "biome check src",
		"check": "tsc --noEmit",
		"test": "vitest run",
		"test:w": "vitest watch",
		"seed:e2e": "tsx e2e/seed.ts",
		"test:e2e": "npm run seed:e2e && npm run playwright",
		"playwright": "playwright test",
		"ci": "./scripts/ci.sh",
		"pages:build": "pnpm next-on-pages",
		"preview": "pnpm pages:build && wrangler pages dev",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
		"generate": "drizzle-kit generate:sqlite --schema=src/adapter/db/schema.ts",
		"migrate:preview": "drizzle-kit migrate --config=drizzle-preview.config.ts",
		"update-meta": "drizzle-kit up:sqlite",
		"studio": "drizzle-kit studio"
	},
	"dependencies": {
		"@effect/opentelemetry": "^0.56.6",
		"@libsql/client": "^0.15.15",
		"@mdx-js/loader": "^3.1.1",
		"@mdx-js/react": "^3.1.1",
		"@next/mdx": "15.5.3",
		"@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
		"@opentelemetry/sdk-trace-base": "^2.1.0",
		"@opentelemetry/sdk-trace-node": "^2.1.0",
		"@opentelemetry/sdk-trace-web": "^2.1.0",
		"@radix-ui/react-avatar": "1.1.10",
		"@radix-ui/react-label": "2.1.7",
		"@radix-ui/react-popover": "1.1.15",
		"@radix-ui/react-slot": "1.2.3",
		"@radix-ui/react-toast": "^1.2.15",
		"better-sqlite3": "^12.2.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"copy-text-to-clipboard": "^3.2.1",
		"drizzle-orm": "^0.44.5",
		"effect": "^3.17.14",
		"lucide-react": "^0.544.0",
		"nanoid": "^5.1.5",
		"next": "15.5.9",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"server-only": "^0.0.1",
		"tailwind-merge": "^2.5.4",
		"tailwindcss-animate": "^1.0.7",
		"temporal-polyfill": "^0.3.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@playwright/test": "1.54.1",
		"@tailwindcss/postcss": "^4.1.13",
		"@tailwindcss/typography": "^0.5.18",
		"@types/better-sqlite3": "^7.6.13",
		"@types/mdx": "^2.0.13",
		"@types/node": "^24.5.2",
		"@types/react": "npm:types-react@19.0.0-rc.1",
		"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
		"drizzle-kit": "^0.31.4",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.13",
		"tsx": "^4.20.5",
		"typescript": "5.9.2",
		"vercel": "^48.1.0",
		"vitest": "^3.2.4"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "npm:types-react@19.0.0-rc.1",
			"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
		}
	}
}
