{
	"name": "groupify",
	"version": "0.1.0",
	"private": true,
	"packageManager": "pnpm@9.12.3",
	"engines": {
		"node": "22"
	},
	"scripts": {
		"dev": "next dev",
		"build": "DB_URL=\"\" next build",
		"start": "next start",
		"lint": "biome check src",
		"check": "tsc --noEmit",
		"test": "vitest run",
		"test:w": "vitest watch",
		"playwright": "playwright test",
		"pages:build": "pnpm next-on-pages",
		"preview": "pnpm pages:build && wrangler pages dev",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
		"generate": "drizzle-kit generate:sqlite --schema=src/adapter/db/schema.ts",
		"migrate:preview": "drizzle-kit migrate --config=drizzle-preview.config.ts",
		"update-meta": "drizzle-kit up:sqlite",
		"studio": "drizzle-kit studio"
	},
	"dependencies": {
		"@effect/opentelemetry": "^0.39.15",
		"@libsql/client": "^0.15.1",
