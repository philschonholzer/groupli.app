		"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.30.6",
		"postcss": "^8.4.49",
		"tailwindcss": "^3.4.15",
		"tsx": "^4.19.2",
		"typescript": "5.6.3",
		"vercel": "^39.0.2",
		"vitest": "^2.1.5"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "npm:types-react@19.0.0-rc.1",
